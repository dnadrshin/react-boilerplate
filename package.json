{
  "name": "react-redux-boilerplate",
  "version": "1.0.0",
  "description": "React redux webpack boilerplate with router",
  "main": "index.js",
  "author": "dnadrshin <dimankaliningrad@gmail.com>",
  "scripts": {
    "test": "jest --env=jsdom",
    "start": "npm run watch-client",
    "watch-client": "better-npm-run watch-client",
    "lint": "eslint app"
  },
  "betterScripts": {
    "watch-client": {
      "command": "node server/index.js",
      "env": {
        "HOST": "0.0.0.0",
        "PORT": 4000,
        "UV_THREADPOOL_SIZE": 100
      }
    },
    "watch-client-win": {
      "command": "node server/index.js",
      "env": {
        "HOST": "127.0.0.1",
        "PORT": 4000,
        "UV_THREADPOOL_SIZE": 100
      }
    }
  },
  "license": "ISC",
  "dependencies": {
    "keymirror": "^0.1.1",
    "path": "^0.12.7",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-redux": "^5.0.5",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.1",
    "redux": "^3.6.0",
    "redux-logger": "^3.0.6",
    "redux-mock-store": "^1.2.3",
    "redux-saga": "^0.15.3"
  },
  "repository": {},
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "better-npm-run": "0.0.15",
    "enzyme": "^2.8.2",
    "eslint": "^3.19.0",
    "eslint-plugin-flowtype": "^2.33.0",
    "eslint-plugin-react": "^7.0.1",
    "flow-bin": "^0.47.0",
    "html-webpack-plugin": "^2.28.0",
    "jest": "^20.0.4",
    "jest-enzyme": "^3.2.0",
    "react-addons-test-utils": "^15.5.1",
    "react-test-renderer": "^15.5.4",
    "webpack": "^2.6.1",
    "webpack-dev-server": "^2.4.5"
  }
}
